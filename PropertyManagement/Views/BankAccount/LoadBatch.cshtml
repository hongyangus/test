@model PropertyManagement.ViewModels.BankAccount.LoadBatchVM
@{
    ViewBag.Title = ViewBag.ReportTitle;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@Html.ActionLink("Back to Bank Statement", "Index")
<h2>@ViewBag.ReportTitle</h2>
@using (Html.BeginForm("Upload", "BankAccount", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()

    <div style="width:220px">
        @Html.LabelFor(x => x.BankAccountName)
        @Html.DropDownListFor(x => x.FinancialBankAccountID, Model.AllBankAccount, "Please select Bank Account", new { @class = "form-control selectpicker" })
    </div>
    <br />

    <div style="width:220px">
        @Html.LabelFor(x => x.AllAccountType)
        @Html.DropDownListFor(x => x.FinancialAccountTypeID, Model.AllAccountType, "Please select  Account Type", new { @class = "form-control selectpicker" })
    </div>
    <br />
    <br />
    <div class="form-group">
        <input type="file" id="dataFile" name="upload" />
    </div>

    <div class="form-group">
        <input type="submit" value="Upload" class="btn btn-default" />
    </div>
}